<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="day4.back.backend.Mapper.ElectricMapper">
	<select id="selectAll" resultType="day4.back.backend.DTO.ElectricityBill">
		<![CDATA[
			SELECT
				HOUR,
				CONSUMPTION,
				UNITPRICE,
				COST
			FROM
				ELECTRIC
		]]>
	</select>
</mapper>
